import java.util.Scanner;

public class factoriales {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        System.out.println("Introduce a number.");
        int num;
        num = s.nextInt();
        int counter = 1;
        int result = 1;

        while(counter <= num)
        {
            result = result* counter;
            counter = counter + 1;
        }
        System.out.println(result);
    }
}
